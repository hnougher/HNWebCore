<!--
	This is just an example of how to use an LDAP 'table' in this OBJ system.
-->
<table class="HNLDAPBasic" LDAPBase="ou=users,o=mojo" read="all" write="" insert="" delete="">
	<field name="dn" type="str" key="unique"/>
	<field name="cn" type="array"/>
	<field name="sn" type="array"/>
	<field name="uid" type="array"/>
	<field name="title" type="array"/>
	<field name="givenname" type="array"/>
	<field name="sn" type="array"/>
	<field name="displayname" type="array"/>
	<field name="employeetype" type="array"/>
	<field name="mail" type="array"/>
	<subtable name="users" table="user" field="ldap_dn"/>
</table>
