<table class="OBJUser" read="user" write="user" insert="user" delete="">
	<field name="userid" type="int" key="auto" read="all" write=""/>
	<field name="username" type="str" max="320" key="unique" read="all"/>
	<field name="password" type="str" max="128" password="1" read="all"/>
	<field name="first_name" type="str" max="40"/>
	<field name="last_name" type="str" max="40"/>
	<field name="gender" type="str" max="6" enum="Male|Female|Other"/>
</table>
